<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Assign Lead to Salesperson</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>jfiatt@invixium.com</assignedTo>
            <assignedToType>User</assignedToType>
            <booleanFilter>1 OR 2 OR 3 OR 4</booleanFilter>
            <criteriaItems>
                <field>Lead.Country</field>
                <operation>contains</operation>
                <value>Anguilla,Antigua and Barbuda,Argentina,Aruba,Bahamas,Barbados,Belize,Bolivia,Bonaire,Brazil,Cayman Islands,Chila,Colombia,Costa Rica,Cuba,Curaçao,Dominica,Dominican Republic,Ecuador,El Salvador,French Guiana,Grenada</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Country</field>
