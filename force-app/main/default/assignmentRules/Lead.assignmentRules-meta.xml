    <assignmentRule>
        <fullName>Import</fullName>
        <active>false</active>
        <ruleEntry>
            <assignedTo>bcooper@invixium.com</assignedTo>
            <assignedToType>User</assignedToType>
            <formula>Country  = &quot;India&quot;</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>ssantos@invixium.com</assignedTo>
            <assignedToType>User</assignedToType>
            <formula>NOT(Country = &quot;United States&quot; || Country = &quot;Canada&quot; ||Country = &quot;India&quot;)</formula>
