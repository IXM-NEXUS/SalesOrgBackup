                <value>Suriname,Trinidad and Tobago,Turks and Caicos Islands,Uruguay,Venezuela,Virgin Islands</value>
            </criteriaItems>
            <template>unfiled$public/LeadsNewassignmentnotificationSAMPLE</template>
        </ruleEntry>
