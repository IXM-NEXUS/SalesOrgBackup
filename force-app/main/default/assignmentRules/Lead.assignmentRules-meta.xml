                <value>Guadeloupe,Guatemala,Guyana,Haiti,Honduras,Jamaica,Martinique,Mexico,Montserrat,Nicaragua,Panama,Paraguay,Peru,Saint Barthélemy</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Country</field>
                <operation>contains</operation>
                <value>Saint Kitts and Nevis,Saint Lucia,Saint Martin,Saint Pierre and Miquelon,Saint Vincent and the Grenadines,Sint Maarten</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Country</field>
